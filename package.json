{
  "name": "devops-commitizen",
  "displayName": "devops-commitizen",
  "description": "vscode customize commitizen support",
  "version": "1.1.1",
  "publisher": "sweetheartjq",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devops-commitizen"
  ],
  "main": "./out/extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/gujunling/devops-commitizen.git"
  },
  "author": {
    "name": "sweetheartjq",
    "email": "sweetheartling@163.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gujunling/devops-commitizen/issues"
  },
  "homepage": "https://github.com/gujunling/devops-commitizen",
  "contributes": {
    "commands": [
      {
        "command": "devops-commitizen",
        "title": "commit",
        "category": "devops-commitizen"
      }
    ],
    "keybindings": [
      {
        "command": "devops-commitizen",
        "key": "ctrl+m",
        "mac": "cmd+m",
        "when": "editorFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.4",
    "vscode": "^1.1.25"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1"
  }
}
